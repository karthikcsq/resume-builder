% Sections formatting
\titleformat{\section}{
  \vspace{-4pt}\scshape\raggedright\large
}{}{0em}{}[\color{black}\titlerule \vspace{-5pt}]

% Ensure that generate pdf is machine readable/ATS parsable
\pdfgentounicode=1

%-------------------------
% Custom commands
\newcommand{\resumeItem}[1]{
  \item\small{
    {#1 \vspace{-1pt}}
  }
}

\newcommand{\resumeSubheading}[4]{
  \vspace{-1pt}\item
    \begin{tabular*}{0.97\textwidth}[t]{l@{\extracolsep{\fill}}r}
      \textbf{#1} & #2 \\
      \textit{\small#3} & \textit{\small #4} \\
    \end{tabular*}\vspace{-5pt}
}

\newcommand{\resumeSubSubheading}[2]{
    \item
    \begin{tabular*}{0.97\textwidth}{l@{\extracolsep{\fill}}r}
      \textit{\small#1} & \textit{\small #2} \\
    \end{tabular*}\vspace{-5pt}
}

\newcommand{\resumeProjectHeading}[2]{
    \item
    \begin{tabular*}{0.97\textwidth}{l@{\extracolsep{\fill}}r}
      \small#1 & #2 \\
    \end{tabular*}\vspace{-5pt}
}

\newcommand{\resumeSubItem}[1]{\resumeItem{#1}\vspace{-3pt}}

\renewcommand\labelitemii{$\vcenter{\hbox{\tiny$\bullet$}}$}

\newcommand{\resumeSubHeadingListStart}{\begin{itemize}[leftmargin=0.15in, label={}]}
\newcommand{\resumeSubHeadingListEnd}{\end{itemize}}
\newcommand{\resumeItemListStart}{\begin{itemize}}
\newcommand{\resumeItemListEnd}{\end{itemize}\vspace{-3pt}}

%-------------------------------------------
%%%%%%  CV STARTS HERE  %%%%%%%%%%%%%%%%%%%%%%%%%%%%


\begin{document}

%----------HEADING----------
\begin{center}
    \textbf{\Huge \scshape <<< name >>>} \\ \vspace{1pt}
    \small <%- if contact -%>
      <%- for c in contact -%>
        <%- if '@' in c -%>\href{mailto:<<< c >>>}{<<< c >>>}<%- else -%><<< c >>><%- endif -%><%- if not loop.last -%> $|$ <%- endif -%>
      <%- endfor -%>
    <%- endif -%>
    <%- if links -%>
      <%- for l in links -%> $|$ 
        \href{<<< l.url >>>}{\underline{<<< l.url | replace('https://','') | replace('http://','') | replace('www.','') >>>}}<%- endfor -%>
    <%- endif -%>
\end{center}


%-----------EDUCATION-----------
\section{Education}
  \resumeSubHeadingListStart
<% for edu in education %>
    \resumeSubheading
      {<<< edu.institution >>>}{<<< edu.location >>>}
      {<<< edu.degree >>><%- if edu.gpa -%>, GPA: <<< edu.gpa >>><%- endif -%>}{<<< edu.dates >>>}
    <%- if edu.coursework -%>
      \resumeItemListStart
        \resumeItem{\textbf{Coursework:} <<< edu.coursework | join(", ") >>>}
      \resumeItemListEnd
    <%- endif -%>
<% endfor %>
  \resumeSubHeadingListEnd


%-----------EXPERIENCE-----------
\section{Experience}
  \resumeSubHeadingListStart
<% for job in experience %>

    \resumeSubheading
      {<<< job.role >>>}{<<< job.start_date >>> -- <<< job.end_date >>>}
      {<<< job.company >>>}{<<< job.location >>>}
      \resumeItemListStart
  <% for bullet in job.bullets %>
        \resumeItem{<<< bullet >>>}
  <% endfor %>
      \resumeItemListEnd
<% endfor %>
      
  \resumeSubHeadingListEnd


%-----------PROJECTS-----------
\section{<%- if section_aliases and section_aliases.projects -%><<< section_aliases.projects >>><%- else -%>Projects<%- endif -%>}
    \resumeSubHeadingListStart
<% for project in projects %>
      \resumeProjectHeading
          {\textbf{<<< project.title >>>} $|$ \emph{<<< project.tools >>>}}{<<< project.date >>>}
          \resumeItemListStart
  <% for item in project.bullets %>
            \resumeItem{<<< item >>>}
  <% endfor %>
          \resumeItemListEnd
<% endfor %>
    \resumeSubHeadingListEnd



%
%-----------PROGRAMMING SKILLS-----------
\section{Technical Skills}
 \begin{itemize}[leftmargin=0.15in, label={}]
    \small{\item{
<% for skill in skills %>
     \textbf{<<< skill.category >>>}{: <<< skill.bullets | join(", ") >>>} <%- if not loop.last -%>\\<%- endif -%>
<% endfor %>
    }}
 \end{itemize}


%-------------------------------------------
\end{document}